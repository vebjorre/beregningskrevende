get_neighbours <- function(l, dim, i){
n <- length(l)
neighbours <- rep(NA, 4)
#c(up,right,down,left)
moves <- c(-dim,1,dim,-1)
valid <- ifelse((i + moves) < 1 | (i + moves) > n, FALSE,TRUE)
return(i+moves[valid])
}
