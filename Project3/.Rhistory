loglik_i <- function(beta,l,dim,i){
neighb <- get_neighbours(l,dim,i)
const <- exp(beta*sum(l[neighb]==0)) + exp(beta*sum(l[neighb]==1))
return (beta*sum(l[i]==l[neighb]) - log(const))
}
